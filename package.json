{
  "name": "sterkbouw-ao-executor",
  "version": "2.0.0",
  "description": "SterkBouw AO Executor met AI Engine integratie",
  "main": "ao.js",
  "scripts": {
    "start": "node ao.js",
    "dev": "nodemon ao.js",
    "dev:ai": "concurrently \"npm run dev\" \"cd src && python -m main\"",
    "test": "jest",
    "test:ai": "cd src && pytest",
    "docker:build": "docker build -t sterkbouw-ao-executor:latest .",
    "docker:ai-build": "docker build -f Dockerfile.ai -t sterkbouw-ai-engine:latest .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "dependencies": {
    "express": "^4.18.2",
    "axios": "^1.6.0",
    "form-data": "^4.0.0",
    "dotenv": "^16.3.1",
    "node-cron": "^3.0.3",
    "winston": "^3.11.0",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "redis": "^4.6.10"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "eslint": "^8.53.0",
    "prettier": "^3.1.0",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
